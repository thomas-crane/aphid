{
  "name": "aphid",
  "version": "0.1.0",
  "description": "A Discord library built for TypeScript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -p .",
    "compile": "tsc -p .",
    "test": "nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/"
    ],
    "exclude": [
      "src/**/index.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  },
  "files": [
    "dist/",
    "license.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomas-crane/aphid.git"
  },
  "keywords": [
    "discord",
    "typescript",
    "library"
  ],
  "author": "tcrane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomas-crane/aphid/issues"
  },
  "homepage": "https://github.com/thomas-crane/aphid#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.10.4",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "discord.js": "^11.4.2",
    "reflect-metadata": "^0.1.13"
  }
}
