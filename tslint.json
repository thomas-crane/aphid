{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "ban-types": [
      true,
      ["Object"],
      ["Number"],
      ["String"]
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
